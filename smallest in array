#include <stdio.h>

int main()
{
    int a[10],i,n,small;
    printf("Size of the array:");
    scanf("%d",&n);
    
    printf("Enter the 5 elements of the array:");
    
    for(i=0;i<n;i++)
    scanf("%d",&a[i]);
    
    small=a[0];
    
    for(i=0;i<n;i++)
    {
    if(small>a[i])
    small=a[i];
    }
    
    printf("The smallest element is %d",small);

    return 0;
}
